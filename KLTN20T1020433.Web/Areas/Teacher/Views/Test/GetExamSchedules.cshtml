@model IEnumerable<KLTN20T1020433.Application.DTOs.TeacherDTOs.GetExamScheduleResponse>
@{
    Layout = null;
}

@foreach (var item in Model)
{
    <div class="card">
        <div class="card-body px-1 py-1">
            <div class="d-flex justify-content-between align-items-center">
                <div>
                    <h5 class="card-title">@item.CourseName</h5>
                    <span class="card-text">Ngày thi: @item.ExamDate</span>
                </div>
                <span class="card-text">@item.ExamDuration Phút</span>
            </div>
        </div>
    </div>
}

